module RefParsers
  class ISIParser < LineParser

    def initialize
      @type_key = "FN"
      @terminator_key = "EF"
      @line_regex =  /^(AB|AF|AP|AR|AU|BE|BN|BP|C1|CA|CL|CR|CY|DE|DI|DT|ED|EM|EP|ER|FN|FU|FX|GA|HO|ID|IS|J9|JI|LA|NR||PD|PA|PG|PI|PN|PT|PU|PY|RID|RP|SE|SI|SN|SO|SP|SU|TC|TI|UT|VL|WC|WP|Z9)  -(\s*(.*))?$/
      @key_regex_order = 1
      @value_regex_order = 2
      @regex_match_length = 3
    end
    
  end
end
